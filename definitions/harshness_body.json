{
    "inputs": { 
      "start_year": "2021",
      "end_year": "2021",
      "data_dir": "/data/",
      "wave_height_thresh": "4",
      "sea_ice_concentration_thresh": "60",
      "formula": "6*S/350 + 2.5*W/110 + 1.5*(I>0.01)*(12 + 2*log10((I/10000)+1e-40))",
      "x_resolution": "0.2",
      "y_resolution": "0.2",
      "lon_min": "{{bbox[0]}}",
      "lat_min": "{{bbox[1]}}",
      "lon_max": "{{bbox[2]}}",
      "lat_max": "{{bbox[3]}}",
      "clean": "True"
    }
  }